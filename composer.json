{
    "name": "your-name-here/Kissagalleria",
    "description": "Kissagalleria plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
				"cakedc/users": "dev-develop",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-google": "^2.0",
        "romano83/cakephp3-media": "dev-master",
        "mirko-pagliai/cakephp-thumber": "^1.0",
        "kareylo/cakephp-comments": "^0.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Kissagalleria\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kissagalleria\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
